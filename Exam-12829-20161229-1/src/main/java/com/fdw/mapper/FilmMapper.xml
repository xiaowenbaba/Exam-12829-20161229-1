<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fdw.mapper.FilmMapper">

	<insert id="insertOneInToFilm" parameterType="com.fdw.entity.Film">
	 insert into  film  (title,description,language_id) 
	 values(
	 	<if test="title!=null and !&quot;&quot;.equals(title)">
	 	#{title}
	 	</if>
	 	,#{description},
	 	<if test="languageId!=null and !&quot;&quot;.equals(languageId)">
	 	#{languageId}
	 	</if>
	 )
	</insert>
</mapper>